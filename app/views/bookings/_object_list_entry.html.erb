<tr id="object_list_entry-<%= object.id %>" class='entry'> 
  <!-- Title -->
  <td>
    <%=    object.name   %>
  </td>
  
  <!-- External Deadline -->
  <td> 
    <%=  object.phone_number  %>
  </td>
  
  <!-- Deliverables -->
  <td> 
    <%= object.seat_category.name %>
  </td>
  
  <!-- Project Memberships -->
  <td>  
    <%= object.number_of_people  %> 
  </td>
  
  <td>
    <%= object.created_at.hour %>:<%= object.created_at.min %>
  </td>
  
  <td> 
    <%= object.booking_code %> 
  </td>
  
  <td> 
    Pending
  </td>
  
  <td> 
   <%= link_to "Ready", send_booking_ready_notification_url, :class => 'btn green mini variable_url_submit' %>  
  </td> 
  
  <td>
    <%= link_to "Close", close_booking_url , :class => 'btn blue mini variable_url_submit' %>  
  </td>
  
  <td>
    <%= link_to "Cancel", cancel_booking_url , :class => 'btn red mini variable_url_submit' %>  
  </td>
</tr>